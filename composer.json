{
  "type": "library",
  "name": "zero-to-prod/url",
  "description": "Parse a url into a class.",
  "keywords": [
    "url",
    "zero-to-prod"
  ],
  "homepage": "https://github.com/zero-to-prod/url",
  "license": "MIT",
  "authors": [
    {
      "name": "David Smith",
      "email": "dave0016@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "zero-to-prod/dynamic-setter": "^71.0",
    "zero-to-prod/data-model": "^71.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Zerotoprod\\Url\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "suggest": {
    "zero-to-prod/transformable": "Transform a class into different types."
  },
  "archive": {
    "exclude": [
      ".*",
      "tests"
    ]
  }
}
